{
  "name": "benefits-ai-chatbot",
  "version": "3.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18 <23"
  },
  "scripts": {
    "guard": "node ./scripts/ci-guard.mjs",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint .",
    "dev": "next dev -p 3000 -H 127.0.0.1",
    "dev:turbo": "next dev --turbo -p 3000 -H 127.0.0.1",
    "verify": "npm run typecheck && npm run lint && npm test && npm run build:vercel",
    "validate:rag": "node ./scripts/validate-rag.mjs",
    "build": "next build",
    "build:vercel": "node ./scripts/prebuild-fix-dirs.mjs && node ./scripts/disable-routes-for-build.cjs && next build && node ./scripts/postbuild-fix-manifests.mjs && node ./scripts/restore-routes-after-build.cjs && node ./scripts/fix-vercel-symlinks.mjs",
    "build:ci": "node ./scripts/ci-guard.mjs && npm run typecheck && npm run lint && next build",
    "start": "next start -p 3000",
    "test": "vitest run",
    "verify:production": "node ./scripts/verify-production-config.mjs",
    "load:test": "k6 run tests/load/k6-rag-scenarios.js",
    "fix:vercel:symlinks": "node ./scripts/fix-vercel-symlinks.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0",
      "git add"
    ]
  },
